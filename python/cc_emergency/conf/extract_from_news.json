{
	"pipeline": [
		{
			"class": "cc_emergency.functional.io.JsonReader",
			"args": ["$input"]
		},
		{
			"class": "cc_emergency.functional.transforms.business.EntityExtractor",
			"kwargs": {
				"keyword_file": "$kw_file",
				"conll_field": "content_corenlp",
				"min_keywords": 1,
				"word_field": 0,
				"type_field": 2,
				"type_regex": "^NNPS?"
			}
		},
		{
			"class": "cc_emergency.functional.io.JsonWriter",
			"args": ["$output"]
		}
	]
}

