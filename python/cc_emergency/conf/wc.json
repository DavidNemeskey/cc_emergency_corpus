{
	"pipeline": [
		{
			"class": "cc_emergency.functional.io.JsonReader",
			"args": ["$input"]
		},
		{
			"class": "cc_emergency.functional.transforms.WC",
			"args": ["text"]
		},
		{
			"class": "cc_emergency.functional.collectors.DictAggregator",
			"args": [["lines", "words", "chars"]]
		}
	],
	"reducer": {
		"class": "cc_emergency.functional.collectors.DictAggregator",
		"args": [["lines", "words", "chars"]]
	}
}

