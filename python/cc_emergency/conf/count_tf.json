{
	"pipeline": [
		{
			"class": "cc_emergency.functional.io.JsonReader",
			"args": ["$input"]
		},
		{
			"class": "cc_emergency.functional.transforms.CoNLLToCounts",
			"args": [
				{
				    "content_corenlp": {
						"column": "lemma",
						"delete": true,
						"new_field": "counts",
						"lower": true,
						"convert_nnps": "NNP"
				    }
				}
			]
		},
		{
			"class": "cc_emergency.functional.transforms.RetainFields",
			"args": [["url", "counts", "content"]]
		},
		{
			"class": "cc_emergency.functional.io.JsonWriter",
			"args": ["$output"]
		}
	]
}
